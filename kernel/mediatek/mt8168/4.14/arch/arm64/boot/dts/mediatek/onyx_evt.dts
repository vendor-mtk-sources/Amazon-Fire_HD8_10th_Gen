				//	pd,country_data = <0xf1 0xf2 0xf3>;
				//};
			};

			dpm_caps {
				local_dr_power;
				local_dr_data;
				//local_ext_power;
				local_usb_comm;
				//local_usb_suspend;
				//local_high_cap;
